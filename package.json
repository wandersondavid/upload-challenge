    "cypress": "^13.6.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "storybook": "^7.6.8",
    "typescript": "^5.2.2",
    "vite": "^5.0.12"
